<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Web_Email_Not_Blank</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK(SuppliedEmail ),
        $Label.TECH_TBL_JP_EXTERNAL_SITE_ID = &apos;tbl-jp&apos;)</errorConditionFormula>
    <errorDisplayField>SuppliedEmail</errorDisplayField>
    <errorMessage>必須入力項目です</errorMessage>
</ValidationRule>
